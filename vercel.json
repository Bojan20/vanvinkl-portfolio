{
  "buildCommand": "cd vanvinkl-slot && npm run build",
  "devCommand": "cd vanvinkl-slot && npm run dev",
  "installCommand": "cd vanvinkl-slot && npm install",
  "outputDirectory": "vanvinkl-slot/.next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}
